{
  "name": "saasbox-bug-reporter",
  "version": "1.0.0",
  "description": "Lightweight embeddable bug reporting widget for SaaS applications. Captures screenshots, console logs, network requests, and system information.",
  "main": "dist/bug-reporter.min.js",
  "unpkg": "dist/bug-reporter.min.js",
  "jsdelivr": "dist/bug-reporter.min.js",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "serve": "python3 -m http.server 8080",
    "prepublishOnly": "npm run build",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "bug",
    "reporting",
    "widget",
    "embeddable",
    "saas",
    "screenshot",
    "error-tracking",
    "feedback",
    "bug-report",
    "error-report",
    "user-feedback"
  ],
  "author": "Sanjay Kumar <hello@iamsanjay.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sanjayio/saasbox.git"
  },
  "bugs": {
    "url": "https://github.com/sanjayio/saasbox/issues"
  },
  "homepage": "https://github.com/sanjayio/saasbox#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "html2canvas": "^1.4.1"
  }
}
